p {
  font-family: articulat-cf, sans-serif;
  font-weight: 600;
  font-style: normal;
}
h1 h2 h3 h4 h5 h6 {
  font-family: articulat-heavy-cf, sans-serif;
  font-weight: 900;
  font-style: normal;
}
a {
  font-family: articulat-cf, sans-serif;
  font-weight: 600;
  text-decoration: none;
  transition: all 0.3s;
  -webkit-transition: all 0.3s;
}
a:hover {
  cursor: pointer;
  text-decoration: none;
}
body {
  margin: 0;
}
.container{
  box-sizing: border-box;
  padding-left: 10px;
  padding-right: 10px;
}
.header_top {
  max-width: 1200px;
  margin: 0 auto;
  display: grid;
  grid-template-columns: 50% 50%;
}
.header_top_left {
  grid-column: 1;
}
.header_top_right {
  grid-column: 2;
  display: flex;
  align-items: center;
}
.header_top_linklist {
  display: flex;
  padding: 14px 0;
}
.header_top_text {
  font-size: 14px;
  font-family: articulat-cf, sans-serif;
  font-weight: 400;
  font-style: normal;
  text-decoration: none;
  padding-right: 10px;
  line-height: 20px;
}
.header_top_linklist .header_top_text:not(:first-child) {
  padding-left: 10px;
  border-left: solid 1.5px #0F2B55;
}
.header_top_text:hover {
  color: black;
}
.blue {
  color: #0F2B55;
}
.header_top_button {
  background-color: black;
  border: none;
  color: white;
  padding: 8px 24px;
  text-align: center;
  text-decoration: none;
  display: inline-block;
  font-size: 12px;
  font-weight: 400;
  margin-left: auto;
}
.header_top_button:hover {
  background-color: white;
  color: black;
  box-shadow: 0 0 0 2px black inset;
}
.header {
  max-width: 1200px;
  margin: 0 auto;
  background-color: #0F2B55;
  display: grid;
  grid-template-columns: 40% 20% 40%;
}
.header_searchbar {
  grid-column: 1;
}
.header_logo_container {
  grid-column: 2;
  display: flex;
  height: 85px;
  margin: 20px 0;
  align-items: center;
  object-fit: contain;
  justify-content: center;
}
.header_left {
  grid-column: 1;
  display: flex;
  align-items: center;
}
.header_searchbar {
  padding-left: 40px;
}
.header_right {
  grid-column: 3;
  display: flex;
  align-items: center;
}
.header_cart_container {
  display: flex;
  align-items: center;
  color: white;
}
.header_cart_link {
  margin-left: auto;
  padding-right: 40px;
}
.header_cart_amount {
  font-size: 16px;
  font-weight: 600;
  padding-right: 8px;
  padding-bottom: 2px;
}
.header_cart_icon {
  fill: white;
  transition: all 0.3s;
}
.header_cart_icon:hover {
  fill: black;
}
.header_account {
  padding-right: 40px;
  fill: white;
  transition: all 0.3s;
}
.header_account:hover {
  fill: #0c0c0c;
}
.searchbar_styling {
  display: flex;
  width: 200px;
  height: 30px;
  border-radius: 15px;
  background-color: white;
  text-decoration: none;
  align-items: center;
}
.searchbar_bar {
  display: flex;
  background: none;
  border: none;
  text-decoration: none;
  font-size: 12px;
  font-weight: 700;
  width: 145px;
  margin: 0 0 0 20px !important;
  padding: 0;
}
.searchbar_button {
  display: flex;
  background: none;
  border: none;
  text-decoration: none;
  width: 20px;
  margin: 0 20px !important 0 0;
  padding: 0;
  cursor: pointer;
}
.header_menu{
  max-width:1200px;
  margin-left:auto;
  margin-right:auto;
  height:60px;
  display: flex;
  align-items:center;
  justify-content:center;
  gap:60px;
}
.header_menu_item{
  font-size:16px;
  font-weight:600;
  color:#0F2B55;
  transition: all 0.3s;
}
.header_menu_item:hover{
  color:black;
}
.header_menu_item.has_children::after{
  content: '>';
  margin-left:6px;
}
.header_menu_item.active::after{
  display:block;
  content: '';
  width:100%;
  height: 2px;
  border-radius:1px;
  background-color:#0F2B55;
}
.footer{
  max-width:1200px;
  margin-left:auto;
  margin-right:auto;
  display:grid;
  grid-template-column:30% 40% 30%;
  align-items:center;
  background-color:#0F2B55;
  padding:60px;
  box-sizing: border-box;
}
.footer_left{
  display:block;
  text-align:left;
}
.footer_info_item{
  display:flex;
  color:white;
  gap:10px;
  margin-top:25px;
}
.footer_info_text{
  font-size: 12px;
  font-family: articulat-cf, sans-serif;
  font-weight: 400;
  font-style: normal;
  transition: all 0.3s;
}
a .footer_info_text:hover{
  color:black;
}