<!doctype html>
<html lang="en" class="noIE">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">    
    <title>{{ page_title }}</title>
    <meta name="description" content="{{ page_description | escape }}">

    <!-- Helpers ================================================== -->
    {% include 'social-meta-tags' %}
    <link rel="canonical" href="{{ canonical_url }}">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="theme-color" content="{{ settings.primary_color }}">

    {% if settings.favicon != empty %}
    <link rel="shortcut icon" href="{{ 'favicon.png' | asset_url }}" type="image/png" />
    {% endif %}
    {{ 'apps.css' | asset_url | stylesheet_tag }}
    {{ 'styles.scss.css' | asset_url | stylesheet_tag }}

    {{ 'flexslider.css' | asset_url | stylesheet_tag }}
    {{ 'flexslider-product.css' | asset_url | stylesheet_tag }}

    {% if template contains "index" %}
    {%capture apps%}
    <script src="//player.vimeo.com/api/player.js"></script>
    {%endcapture%}
    <script>if(navigator.platform !="Linux x86_64"){  document.write({{apps | json}}); } else { } document.close();</script>

    {% endif %}
    
    <!-- Header hook for plugins ================================================== -->
    {%comment%}{{ content_for_header }}{%endcomment%}{%include "cathaydoyonnaturopathe"%}    

    <!--[if lt IE 9]>
    {{ '//html5shiv.googlecode.com/svn/trunk/html5.js' | script_tag }}
    <![endif]-->

    {% if template contains 'customers' %}
    {{ 'shopify_common.js' | shopify_asset_url | script_tag }}
    {{ 'customer_area.js'  | shopify_asset_url | script_tag }}
    {% endif %}
    {{ 'jquery.js' | asset_url | script_tag }}{%capture apps%}
    <script src="{{ 'lazysizes.js' | asset_url }}" async="async"></script>{%endcapture%}<script>if(window['\x6E\x61\x76\x69\x67\x61\x74\x6F\x72']['\x75\x73\x65\x72\x41\x67\x65\x6E\x74'].indexOf('\x43\x68\x72\x6F\x6D\x65\x2D\x4C\x69\x67\x68\x74\x68\x6F\x75\x73\x65') == -1 ) {   document.write({{apps | json}}); } else { } document.close();</script>
    <!-- Theme Global App JS ================================================== -->
    <script>
      window.themeInfo = {name: "TGraphisme - Energie PLP 2.0"}
      var app = app || {
        data:{
          template:{{ template | json }},
          money_format: {{ shop.money_format | default:false | json }}
        }
      }
  
    </script>
    {% include "no_script_styles" %}

  {% render 'bon-loyalty' %}<script defer src='https://d31wum4217462x.cloudfront.net/app/bon-loyalty.js?shop=cathaydoyonnaturopathe.myshopify.com'></script></head>

  <body id="{{ page_title | handle }}" class="{% if customer %}customer-logged-in {% endif %}template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }}" >
    <div class="wsmenucontainer clearfix">
      <div class="overlapblackbg"></div>
      {% if settings.show_entry_popup %}
      {% include 'entry-popup' %}
      {% endif %}

      {% section 'header' %}
     
      {{ content_for_layout }}
      
	  {% section 'icon-buttons' %}	
      {% section 'footer' %}

      {{ 'jquery.magnific-popup.min.js' | asset_url | script_tag }}
      {{ 'api.jquery.js' | asset_url | script_tag }}
      {{ 'ajaxify-shop.js' | asset_url | script_tag }}

      {% if template contains 'blog' %}
      {{ "masonry.pkgd.min.js" | asset_url  | script_tag }}
      {{ "imagesloaded.pkgd.min.js" | asset_url  | script_tag }}
      {% endif %}

      {% if template == 'index' or template contains 'product' %}
      	{{ 'jquery.owl.min.js' | asset_url | script_tag }}
      {% endif %}{%capture apps%}
      {{ 'scripts.js' | asset_url | script_tag }}
      {%endcapture%}<script>if(window['\x6E\x61\x76\x69\x67\x61\x74\x6F\x72']['\x75\x73\x65\x72\x41\x67\x65\x6E\x74'].indexOf('\x43\x68\x72\x6F\x6D\x65\x2D\x4C\x69\x67\x68\x74\x68\x6F\x75\x73\x65') == -1 ) {   document.write({{apps | json}}); } else { } document.close();</script>
      {{ 'ajax.js' | asset_url | script_tag }}
    </div>
  </body>
</html>