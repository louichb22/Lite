      {% if section.settings.address == blank %}{% else %}<item class="footer_info_item"><i class="far fa-location-dot footer_icon"></i><span class="footer_text">{{ section.settings.address }}</span></item>{% endif %}
      {% if section.settings.phone == blank %}{% else %}<a href="tel:{{ section.settings.phone }}"<item class="footer_info_item"><i class="far fa-phone"></i><span class="footer_text">{{ section.settings.phone }}</span></item></a>{% endif %}
