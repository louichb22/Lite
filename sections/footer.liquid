{% schema %}
{
    "name": "Footer",
    "tag": "footer",
    "settings": [
        {
        "type": "header",
        "content": "Footer"
        },
      {
        "type": "text",
        "id": "address",
        "label": "Adresse"
      },
      {
        "type": "text",
        "id": "phone",
        "label": "Téléphone"
      },
      {
        "type": "text",
        "id": "email",
        "label": "Email"
      },
      {
        "type": "textarea",
        "id": "middle_text",
        "label": "Texte au centre"
      }
    ]
}
{% endschema %}